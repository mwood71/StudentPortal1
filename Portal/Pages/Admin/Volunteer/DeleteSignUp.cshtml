@page
@model Portal.Pages.Admin.Volunteer.DeleteSignUpModel
@{
    ViewData["Title"] = "Admin - Volunteer";
}

<form method="post">
    <div class="container">
        <input asp-for="Volunteer.Id" type="hidden"/>
        <div class="form-group">
            <label>Name:</label>
            <input asp-for="Volunteer.Name" class="form-control"/>

        </div>
        <div class="form-group">
            <input class="btn btn-danger" type="submit" value="Delete" />

        </div>

    </div>

</form>