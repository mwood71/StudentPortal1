@page
@model Portal.Pages.Admin.MeetAMember.CreateModel
@{
    ViewData["Title"] = "Admin - Meet A Member";
}

<form method="post">
    <div class="container">
        <div class="form-group">
            <label>Title:</label>
            <input asp-for="MeetAMember.Title" class="form-control" />

        </div>
        <div class="form-group">
            <label>Author:</label>
            <input asp-for="MeetAMember.Author" class="form-control" />

        </div>
        <div class="form-group">
            <label>PictureURL:</label>
            <input asp-for="MeetAMember.PictureURL" class="form-control" />

        </div>
        <div class="form-group">
            <label>Paragraph 1:</label>
            <textarea rows="3" asp-for="MeetAMember.Para1" class="form-control" ></textarea>

        </div>
        <div class="form-group">
            <label>Paragraph 2:</label>
            <textarea rows="3" asp-for="MeetAMember.Para2" class="form-control"></textarea>

        </div>
        <div class="form-group">
            <label>Paragraph 3:</label>
            <textarea rows="3" asp-for="MeetAMember.Para3" class="form-control"></textarea>

        </div>
        <div class="form-group">
            <label>Paragraph 4:</label>
            <textarea rows="3" asp-for="MeetAMember.Para4" class="form-control"></textarea>

        </div>
        <div class="form-group">
            <label>Paragraph 5:</label>
            <textarea rows="3" asp-for="MeetAMember.Para5" class="form-control"></textarea>

        </div>
        <div class="form-group">
            <label>Paragraph 6:</label>
            <textarea rows="3" asp-for="MeetAMember.Para6" class="form-control"></textarea>

        </div>
        <div class="form-group">
            <label>Paragraph 7:</label>
            <textarea rows="3" asp-for="MeetAMember.Para7" class="form-control"></textarea>

        </div>
        <div class="form-group">
            <label>Paragraph 8:</label>
            <textarea rows="3" asp-for="MeetAMember.Para8" class="form-control"></textarea>

        </div>
        <div class="form-group">
            <label>Paragraph 9:</label>
            <textarea rows="3" asp-for="MeetAMember.Para9" class="form-control"></textarea>

        </div>
        <div class="form-group">
            <label>Paragraph 10:</label>
            <textarea rows="3" asp-for="MeetAMember.Para10" class="form-control"></textarea>

        </div>
        <div class="form-group">
            <input class="btn btn-primary" type="submit" value="Add"/>

        </div>

    </div>

</form>
