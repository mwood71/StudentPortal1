@page
@model Portal.Pages.Admin.Meeting.DeleteModel
@{
    ViewData["Title"] = "Admin - Meetings";
}


<form method="post">

    <div class="container">
        <input asp-for="Meeting.Id" type="hidden" disabled/>
        <div class="form-group">
            <label>Name:</label>
            <input asp-for="Meeting.Name" class="form-control" disabled />

        </div>
        <div class="form-group">
            <label>Day of the week:</label>
            <input asp-for="Meeting.DayOfWeek" class="form-control" disabled />

        </div>
        <div class="form-group">
            <label>Start time:</label>
            <input id="timepicker" asp-for="Meeting.StartTime" class="form-control" disabled />

        </div>
        <div class="form-group">
            <label>End time:</label>
            <input id="timepick" asp-for="Meeting.EndTime" class="form-control" disabled/>

        </div>
        <div class="form-group">
            <label>Address:</label>
            <input asp-for="Meeting.Address" class="form-control" disabled />

        </div>
        <div class="form-group">
            <label>City:</label>
            <input asp-for="Meeting.City" class="form-control" disabled />

        </div>
        <div class="form-group">
            <label>Zip:</label>
            <input asp-for="Meeting.Zip" class="form-control" disabled />

        </div>
        <div class="form-group">
            <label>State:</label>
            <input asp-for="Meeting.State" class="form-control" disabled />

        </div>
        <div class="form-group">
            <input type="submit" value="Delete" class="btn btn-danger" />

        </div>

    </div>

</form>