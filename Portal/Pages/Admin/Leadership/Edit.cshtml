@page
@model Portal.Pages.Admin.Leadership.EditModel
@{
    ViewData["Title"] = "Admin - Leadership";
}

<form method="post">
    <div class="container">

        <input type="hidden" asp-for="Leadership.Id" />
        <div class="form-group">
            <label>Name:</label>
            <input asp-for="Leadership.Name" class="form-control" />

        </div>
        <div class="form-group">
            <label>Position:</label>
            <input asp-for="Leadership.Position" class="form-control"  />

        </div>
        <div class="form-group">
            <label>Picture URL:</label>
            <input asp-for="Leadership.url" class="form-control" />

        </div>
        <div class="form-group">
            <input class="btn btn-success" type="submit" value="Submit Edit" />

        </div>

    </div>

</form>
