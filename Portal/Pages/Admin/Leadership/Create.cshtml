@page
@model Portal.Pages.Admin.Leadership.CreateModel
@{
    ViewData["Title"] = "Admin - Leadership";
}


<form method="post">
    <div class="container">
        <div class="form-group">
            <label>Name:</label>
            <input asp-for="Leadership.Name" class="form-control" />

        </div>
        <div class="form-group">
            <label>Position:</label>
            <input asp-for="Leadership.Position" class="form-control" />

        </div>
        <div class="form-group">
            <label>Picture URL:</label>
            <input asp-for="Leadership.url" class="form-control" />

        </div>
        <div class="form-group">
            <input class="btn btn-primary" type="submit" value="Add"/>

        </div>

    </div>

</form>